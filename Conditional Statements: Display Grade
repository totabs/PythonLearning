score = input("Enter Score: ")
gr = float(score)

#The conditions with which each letter or error message is displayed
if gr > 1.0:
    print("Error, type a number equal or inferior to 1.0.")
elif gr >= 0.9:
    print('A')
elif gr >= 0.8:
    print('B')
elif gr >= 0.7:
    print('C')
elif gr >= 0.6:
    print('D')
elif gr < 0.0:
    print("Error, type a number between 0.0 and 1.0.")
elif gr < 0.6:
    print('F')
